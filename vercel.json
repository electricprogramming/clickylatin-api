{
  "routes": [
    {
      "src": "/api/*",
      "methods": ["GET"],
      "dest": "/api/get-handler"
    },
    {
      "src": "/api/*",
      "methods": ["POST"],
      "dest": "/api/post-handler"
    },
    {
      "src": "/api/*",
      "methods": ["PUT"],
      "dest": "/api/put-handler"
    },
    {
      "src": "/api/*",
      "methods": ["DELETE"],
      "dest": "/api/delete-handler"
    }
  ]
}
